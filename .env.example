# 应用配置
NODE_ENV=development
PORT=7001

# 应用信息
APP_NAME=NestJS API Server
APP_VERSION=1.0.0
APP_DESCRIPTION=A robust NestJS API server with best practices

# API 配置
API_PREFIX=api
API_VERSION=v1

# Swagger 配置
SWAGGER_ENABLED=true
SWAGGER_PATH=api-docs

# 日志配置
LOG_LEVEL=debug
LOG_DIR=logs
LOG_MAX_FILES=14d
LOG_MAX_SIZE=20m
